{
    "sourceMaps": false,
    "module": {
        "type": "commonjs",
        "strict": false,
        "strictMode": true,
        "lazy": false,
        "noInterop": false
    },
    "jsc": {
        "externalHelpers": true,
        "target": "es2019",
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "decorators": true
        },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        }
    }
}
